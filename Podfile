source 'https://github.com/CocoaPods/Specs.git'
platform :ios, '10.0'
inhibit_all_warnings!
use_frameworks!

def pods
    #网络请求
    pod 'Alamofire', '~> 4.7.3'	
    #网络状态监测
    pod 'RealReachability'
  
    #Json解析
    pod 'ObjectMapper'
    pod 'SwiftyJSON'
    
end

target 'Carro' do
    pods
end
